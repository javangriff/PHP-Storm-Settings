<application>
  <component name="ApplicationTasksOptions">
    <global-watchers>
      <TaskOptions>
        <option name="arguments" value="--fix $FileName$" />
        <option name="checkSyntaxErrors" value="true" />
        <option name="description" />
        <option name="exitCodeBehavior" value="NEVER" />
        <option name="fileExtension" value="vue" />
        <option name="immediateSync" value="false" />
        <option name="name" value="Eslint Fix" />
        <option name="output" value="" />
        <option name="outputFilters">
          <array />
        </option>
        <option name="outputFromStdout" value="false" />
        <option name="program" value="$ProjectFileDir$/node_modules/eslint/bin/eslint.js" />
        <option name="runOnExternalChanges" value="false" />
        <option name="scopeName" value="All Places" />
        <option name="trackOnlyRoot" value="false" />
        <option name="workingDir" value="$FileDir$" />
        <envs />
      </TaskOptions>
    </global-watchers>
  </component>
</application>